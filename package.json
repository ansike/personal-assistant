{
  "name": "PersonalAssistant",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "1--": "启动android开发模式",
    "android": "react-native run-android",
    "2--": "启动ios开发模式",
    "start": "react-native start",
    "3--": "清除cache模式 启动react native",
    "cr": "react-native start --reset-cache",
    "4--": "清除android缓存",
    "ac": "cd android && ./gradlew clean",
    "5--": "编译debug产物",
    "ad": "cd android && ./gradlew assembleDebug",
    "6--": "编译release产物",
    "ar": "cd android && ./gradlew assembleRelease",
    "7--": "启动接口服务",
    "server": "cd server && yarn dev",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^12.0.0",
    "@react-native-community/cli-platform-android": "^12.0.0",
    "@react-native/gradle-plugin": "^0.73.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
